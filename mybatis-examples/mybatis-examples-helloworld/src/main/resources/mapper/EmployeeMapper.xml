<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wusuiwei.mybatis.helloworld.EmployeeMapper">
    <select id="selectOne" resultType="com.wusuiwei.mybatis.helloworld.Employee">
        select *
        from employee
        where id = #{id}
    </select>
</mapper>